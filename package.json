{
	"name": "motion-tools",
	"private": true,
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"dev": "tsx server/check-bun && concurrently \"vite dev\" \"bun run server/server.ts\"",
		"build": "vite build",
		"server": "echo \"This command is now lumped into pnpm dev and is no longer required\"",
		"preview": "vite preview",
		"prepare": "svelte-kit sync || echo ''",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"format": "prettier --write .",
		"lint": "prettier --check . && eslint .",
		"test:unit": "vitest",
		"test:client": "go test ./... -count=1",
		"test": "npm run test:unit -- --run && npm run test:e2e",
		"test:e2e": "playwright test",
		"model-pipeline:run": "node scripts/model-pipeline.js"
	},
