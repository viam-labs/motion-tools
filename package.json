		"tsx": "4.20.1",
		"typescript": "5.8.3",
		"typescript-eslint": "8.34.0",
		"vite": "6.3.5",
		"vite-plugin-devtools-json": "0.2.0",
		"vite-plugin-mkcert": "1.17.8",
		"vitest": "3.2.3"
	},
	"peerDependencies": {
		"@dimforge/rapier3d-compat": ">=0.17",
		"@tanstack/svelte-query": ">=5",
		"@threlte/core": ">=8",
		"@threlte/extras": ">=9",
